<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Sports Challenge Login</title>


    
   

    <!-- Bootstrap -->
    <link href="/lib/bootstrap-3.3.2-dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- CustomCSS -->
    <link rel="stylesheet/less" type="text/css" href="/stylesheets/init.less" />
    <link rel="stylesheet/less" type="text/css" href="/stylesheets/index.less" />
    <link rel="stylesheet/less" type="text/css" href="/stylesheets/login.less" />

      <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
      <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
      <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
      <![endif]-->


    <script src="/lib/angular.min.js"></script>
    
    <script src="/lib/jquery-1.11.2.min.js"></script>
    <script src="/lib/bootstrap-3.3.2-dist/js/bootstrap.min.js"></script>
    <script src="/lib/less.min.js"></script>
   
    <script src="/lib/sportschallenge.js"></script>
   
    <script src="/socket.io/socket.io.js"></script>

    <!-- 소켓 팩토리 -->
    <script>
      var socketCallback;

      var socket = io.connect();
      socket.on('message', function(data){
        socketCallback && socketCallback(data);
      });

    </script>


</head>
<body ng-app="SportsChallenge">

  <sc-header></sc-header>
  


  <div id="wrapper">
    <div id="loginHeader">
      <h1>로그인</h1>
      <h2><STRONG>Challenge</STRONG>에 오신 것을 환영합니다.</h2>
    </div>
    <div id="loginContent">
      <div id="loginForm">
        <form class="form-horizontal" method="post">
          <div class="form-group">
            <label for="inputEmail3" class="col-sm-2 control-label">Email</label>
            <div class="col-sm-10">
              <input type="text" class="form-control" id="inputEmail3" placeholder="Id" name="login">
            </div>
          </div>
          <div class="form-group">
            <label for="inputPassword3" class="col-sm-2 control-label">Password</label>
            <div class="col-sm-10">
              <input type="password" class="form-control" id="inputPassword3" placeholder="Password" name="password">
            </div>
          </div>
          <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
              <div class="checkbox">
                <label>
                  <input type="checkbox"> Remember
                </label>
              </div>

            </div>
          </div>
          <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
              <button type="submit" class="btn btn-default">Sign in</button>
            </div>
          </div>
        </form>
      </div>
      <div id="loginAdd">
        <a href="#">광고 480X225</a>

      </div>
    </div>
  </div>
  
  <sc-footer></sc-footer>


</body>
</html>