    <script src="/socket.io/socket.io.js"></script>

    <!-- 소켓 팩토리 -->
    <script>
      var socketCallback;

      var socket = io.connect();
      socket.on('message', function(data){
        socketCallback && socketCallback(data);
      });

    </script>

    <script>
      $(document).ready(function(){

        $('.carousel').carousel({
          interval: 3000
        })

        var userName = $('#isLogin').html();
        

        if(userName != "Please login"){
          $('#login').hide();
          $('#span1').hide();
          $('#register').hide();
          $('#span2').hide();

          $('#logout').show();
        } else{
          $('#logout').hide();
          $('#span3').hide();
        }

        $('#logout').click(function(){
          $('#login').show();
          $('#span1').show();
          $('#register').show();
          $('#span2').show();
          $('#logout').hide(); 
        });


        $('#newChalbtn').click(function(){
          var userName = $('#isLogin').html();
          if(userName == "Please login"){
            alert('로그인 해주세요');
          }
        });



      });




      
    </script>


